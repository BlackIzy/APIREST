<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.entities.ApplicableRolesEntity">
        <table name="APPLICABLE_ROLES" schema="information_schema" catalog=""/>
        <attributes>
            <basic name="grantee">
                <column name="GRANTEE" precision="-1"/>
            </basic>
            <basic name="roleName">
                <column name="ROLE_NAME" precision="-1"/>
            </basic>
            <basic name="isGrantable">
                <column name="IS_GRANTABLE" precision="-1"/>
            </basic>
            <basic name="isDefault">
                <column name="IS_DEFAULT" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
