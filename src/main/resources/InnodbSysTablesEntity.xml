<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.entities.InnodbSysTablesEntity">
        <table name="INNODB_SYS_TABLES" schema="information_schema" catalog=""/>
        <attributes>
            <basic name="tableId">
                <column name="TABLE_ID" precision="-1"/>
            </basic>
            <basic name="name">
                <column name="NAME" precision="-1"/>
            </basic>
            <basic name="flag">
                <column name="FLAG" precision="-1"/>
            </basic>
            <basic name="nCols">
                <column name="N_COLS" precision="-1"/>
            </basic>
            <basic name="space">
                <column name="SPACE" precision="-1"/>
            </basic>
            <basic name="rowFormat">
                <column name="ROW_FORMAT" nullable="false" precision="-1"/>
            </basic>
            <basic name="zipPageSize">
                <column name="ZIP_PAGE_SIZE" precision="-1"/>
            </basic>
            <basic name="spaceType">
                <column name="SPACE_TYPE" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
